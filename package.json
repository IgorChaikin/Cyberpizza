    "start": "node server/index.js",
    "build": "webpack --mode production",
    "test": "jest",
    "server:watch": "nodemon server/index.js",
    "client:watch": "webpack serve",
    "lint": "eslint \"{,!(node_modules|dist|stories|storybook-static)/**/}*.+(js|jsx)\" --fix",
    "format": "prettier --write \"{,!(node_modules|dist|stories|storybook-static)/**/}*.+(js|jsx|json|css|scss|md)\"",
    "prepare": "husky install",
    "deploy": "git push heroku -f production:master",
    "commit-deploy": "git add . && git commit -m \"deploy on heroku\" && npm run deploy",
    "seed": "node server/scripts/db.init.js",
    "storybook": "start-storybook -s ./stories/assets,./static/svg,./static/media -p 6006",
    "build-storybook": "build-storybook"
  },
